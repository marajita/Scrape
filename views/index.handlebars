<body>
  <div class="container">
    <ul id="article-list" class="list-group">
      {{#each articles}}
      <li data-id="{{this.id}}" class="list-group-item">
        <a href="{{this.link}}">{{this.title}}</a>
        <p>{{this.description}}</p>
        <p>{{this.datePublished}}</p>
        <button class="btn btn-default float-right delete">Save</button>
      </li>
      {{/each}}
    </ul>

  </div>
</body>